extends layouts/frontend

block content
    div
        a(href="/materiaux" style="color:brown") Retour aux matériaux
    .ui.middle.aligned.center.aligned.grid
        .six.wide.column
            h1 Tous les meubles composés de plastique

    each data in datas
        h3=data.name
        p=data.description
        img(src="img/"+ data.img, alt=data.type+" "+data.name style="width:200px;height:300px;")